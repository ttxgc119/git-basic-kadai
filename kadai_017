# 商品データのクラス定義
class Human:
    def __init__(self, name, age):
        # 初期化
        self.name = name
        self.age = age
    # 名前を設定するメソッド
    def printinfo(self):
        print(f"名前: {self.name}, 年齢: {self.age}")

    
# Humanクラスのインスタンス化
Person = Human("侍太郎", 39)

#Personメソッドの呼び出し
Person.printinfo()